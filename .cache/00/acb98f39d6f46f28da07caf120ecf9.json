gin-bottom: 1vh;\n}\n\n.score {\n  font-size: 8vh;\n  font-weight: 300;\n  color: #09108c;\n  margin-bottom: auto;\n}\n\n.player--active {\n  background-color: rgba(82, 55, 236, 0.836);\n}\n.player--active .name {\n  font-weight: 700;\n}\n.player--active .score {\n  font-weight: 400;\n}\n\n.player--active .current {\n  opacity: 1;\n}\n\n.current {\n  background-color: #3b22c7;\n  opacity: 0.8;\n  border-radius: 9px;\n  color: #fff;\n  width: 65%;\n  padding: 2vh;\n  text-align: center;\n  transition: all 0.75s;\n}\n\n.current-label {\n  text-transform: uppercase;\n  margin-bottom: 1vh;\n  font-size: 2vh;\n  color: #ddd;\n}\n\n.current-score {\n  font-size: 4.5vh;\n}\n\n/* ABSOLUTE POSITIONED ELEMENTS */\n.btn {\n  position: absolute;\n  left: 50%;\n  transform: translateX(-50%);\n  color: #444;\n  background: none;\n  border: none;\n  font-family: inherit;\n  font-size: 1.8vh;\n  text-transform: uppercase;\n  cursor: pointer;\n  font-weight: 400;\n  transition: all 0.2s;\n\n  background-color: white;\n  background-color: rgba(255, 255, 255, 0.6);\n  backdrop-filter: blur(10px);\n\n  padding: 0.7vh 2.5vh;\n  border-radius: 50vh;\n  box-shadow: 0 1.75vh 3.5vh rgba(0, 0, 0, 0.1);\n}\n\n.btn::first-letter {\n  font-size: 2.4vh;\n  display: inline-block;\n  margin-right: 0.7vh;\n}\n\n.btn--new {\n  top: 4vh;\n}\n.btn--roll {\n  top: 65%;\n}\n.btn--hold {\n  top: 75%;\n}\n\n.btn:active {\n  transform: translate(-50%, 3px);\n  box-shadow: 0 1vh 2vh rgba(0, 0, 0, 0.15);\n}\n\n.btn:focus {\n  outline: none;\n}\n\n.dice {\n  position: absolute;\n  left: 50%;\n  top: 40%;\n  transform: translateX(-50%);\n  height: 10vh;\n  box-shadow: 0 2vh 5vh rgba(0, 0, 0, 0.2);\n}\n\n.player--winner {\n  background-color: #57e974;\n}\n\n.player--winner .name {\n  font-weight: 700;\n  color: #2132e6;\n}\n\n.hidden {\n  display: none;\n}\n"},"lineCount":null}},"error":null,"hash":"0274d534984839c86713cc73f163eace","cacheData":{"env":{}}}